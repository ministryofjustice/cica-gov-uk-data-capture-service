{
    "name": "data-capture-service",
    "version": "9.6.3",
    "engines": {
        "npm": ">=10.8.2",
        "node": ">=22.8.0"
    },
    "scripts": {
        "start": "node ./bin/www",
        "start:dev": "nodemon -L -e .js,.json,.njk,.yml --ignore openapi/openapi.json --ignore openapi/openapi-admin.json --exec npm run build:run:dev",
        "openapi:build-public": "speccy lint openapi/src/openapi-src.json -j && speccy resolve ./openapi/src/openapi-src.json -j | yaml2json --pretty --indentation 4 --save - > ./openapi/openapi.json && node ./openapi/src/dereference-openapi.js",
        "openapi:build-admin": "speccy lint openapi/src/openapi-src-admin.json -j && speccy resolve ./openapi/src/openapi-src-admin.json -j | yaml2json --pretty --indentation 4 --save - > ./openapi/openapi-admin.json && node ./openapi/src/dereference-openapi-admin.js",
        "openapi:build": "npm run openapi:build-public && npm run openapi:build-admin",
        "build:run:dev": "npm run openapi:build && node --inspect=0.0.0.0:9229 ./bin/www",
        "pretestx": "eslint .",
        "test": "jest",
        "coverage": "jest --coverage",
        "dump-eslint-conf": "eslint --print-config ./app.js > ./stuff/eslint-conf.json",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.345.0",
        "@aws-sdk/client-sqs": "^3.345.0",
        "ajv": "^6.12.6",
        "ajv-errors": "^1.0.1",
        "ajv-formats-mobile-uk": "github:CriminalInjuriesCompensationAuthority/ajv-formats-mobile-uk#v1.0.0",
        "cookie-parser": "~1.4.3",
        "debug": "~2.6.9",
        "express": "^4.18.2",
        "express-jwt": "^8.0.0",
        "express-jwt-authz": "^2.4.1",
        "express-openapi-validator": "^5.3.5",
        "got": "^11.8.5",
        "helmet": "^3.21.2",
        "json-translator": "github:CriminalInjuriesCompensationAuthority/json-translator#v2.0.0",
        "jsonpointer": "^5.0.0",
        "lodash.merge": "^4.6.2",
        "moment-timezone": "^0.5.33",
        "notifications-node-client": "^7.0.0",
        "pg": "^8.7.3",
        "pino-http": "^5.5.0",
        "pino-std-serializers": "^6.2.2",
        "q-expressions": "github:CriminalInjuriesCompensationAuthority/q-expressions",
        "q-router": "github:CriminalInjuriesCompensationAuthority/q-router#v3.1.1",
        "q-templates-application": "github:CriminalInjuriesCompensationAuthority/q-templates-application#cya-pdf-confirmation-heading-changes",
        "semver": "^7.5.4",
        "swagger-ui-express": "^4.6.3",
        "uuid": "^3.3.2",
        "verror": "^1.10.0"
    },
    "devDependencies": {
        "aws-sdk-client-mock": "^2.1.1",
        "eslint": "^8.0.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jest": "^23.3.0",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.0.3",
        "jest": "^28.1.3",
        "jest-json-schema": "^2.0.2",
        "json-schema-ref-parser": "^6.1.0",
        "lint-staged": "^11.0.0",
        "nodemon": "^3.0.0",
        "pino-pretty": "^10.3.0",
        "prettier": "^1.19.1",
        "speccy": "github:wework/speccy#740d19d88935db7735250c16abc2ad09256b5854",
        "supertest": "^4.0.2",
        "yamljs": "^0.3.0"
    },
    "license": "MIT"
}
